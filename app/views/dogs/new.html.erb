<h1>Creating New Dog Profile!</h1>

<%= form_for :dog, url: dogs_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :sex %><br>
    <%= f.select :sex,  ["Not telling","Male", "Female"] %>
  </p>
 
 <p>
    <%= f.label :age %><br>
    <%= f.text_field :age %>
  </p>
  
  <p>
    <%= f.label :bio %><br>
    <%= f.text_field :bio %>
  </p>
  
 
  <p>
    <%= f.submit %>
  </p>
<% end %>