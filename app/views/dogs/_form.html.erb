<%= form_for :dog, url: dogs_path do |f| %>
  <h3>Account info</h3>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>
  
  <p>
    <%= f.label :password %><br>
    <%= f.text_field :password %>
  </p>
   <h3>Dog info</h3>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :sex %><br>
    <%= f.select :sex,  ["Not telling", "Male", "Female"] %>
  </p>
 
 <p>
    <%= f.label :age %><br>
    <%= f.text_field :age %>
  </p>
  
  <p>
    <%= f.label :bio %><br>
    <%= f.text_field :bio %>
  </p>
  
 
  <p>
    <%= f.submit %>
  </p>
<% end %>